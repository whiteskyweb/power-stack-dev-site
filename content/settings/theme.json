{
  "color": {
    "black": "#393768",
    "white": "#FFFFFF",
    "primary": "#FFC800",
    "secondary": "#7E7CAE"
  },
  "easing": "cubic-bezier(0.215, 0.610, 0.355, 1.000)",
  "breakpoints": {
    "small": "600px",
    "medium": "1200px",
    "large": "1600px",
    "huge": "2200px"
  },
  "radius": {
    "small": "23px"
  },
  "header": {
    "overline": false,
    "transparent": false,
    "height": "5rem",
    "underline": true
  },
  "hero": {
    "image": "../images/Hero.png",
    "large": true,
    "overlay": true,
    "overlap": "2em",
    "parallax": true
  },
  "typography": {
    "uppercaseH2": false
  },
  "menu": {
    "style": "pill"
  },
  "__gatsby_resolved": {
    "fileRelativePath": "/content/settings/theme.json"
  }
}